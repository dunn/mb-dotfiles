#!/usr/bin/env bash

alias ebug="/usr/local/bin/emacs --debug-init"
alias gomacs="/usr/local/bin/emacs --daemon"
alias emcs="/usr/local/bin/emacsclient -c -a emacs"
alias emac="/usr/local/bin/emacsclient -t -a emacs"
alias emcas=emcs
alias emca=emac
alias semac="sudo /usr/local/bin/emacsclient -t -a emacs"
alias semacs="sudo /usr/local/bin/emacsclient -c -a emacs"
alias boop="/usr/local/bin/emacsclient -c . &"
